{
  "name": "@champagne/guards",
  "private": true,
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts",
    "tests",
    "tools",
    "Guards_Audit.json",
    "Guards_Audit_Report.md",
    "Guards_Baseline_Report.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "guard:hero": "pnpm run guard:hero-core && pnpm run guard:sacred-hero-lock",
    "guard:hero-core": "node scripts/guard-hero.mjs",
    "guard:sacred-hero-lock": "node scripts/guard-sacred-hero-lock.mjs",
    "guard:rogue-hex": "node scripts/guard-rogue-hex.mjs",
    "guard:canon": "node scripts/guard-canon.mjs",
    "guard:hero-freeze": "node scripts/guard-hero-freeze.mjs",
    "guard:manifest": "node scripts/guard-manifest-sync.mjs",
    "guard:manifests": "pnpm run guard:manifest",
    "guard:brand-structure": "node scripts/brand-structure-guard.cjs",
    "guard:no-_imports-runtime": "node scripts/guard-no-imports-from-_imports.mjs",
    "guard:no-book": "node scripts/guard-no-book.mjs",
    "guard:cta-single-terminal": "node scripts/guard-cta-single-terminal.cjs",
    "guard:cta-contract": "bash ./scripts/run-cta-contract.sh",
    "guard:token-binding": "node scripts/guard-token-binding.mjs",
    "guard:smh-treatment-layouts": "node scripts/guard-smh-treatment-layouts.mjs",
    "guard:all": "pnpm run guard:hero && pnpm run guard:rogue-hex && pnpm run guard:canon && pnpm run guard:token-binding && pnpm run guard:hero-freeze && pnpm run guard:manifest && pnpm run guard:no-_imports-runtime && pnpm run guard:brand-structure && pnpm run guard:no-book && pnpm run guard:cta-single-terminal && pnpm run guard:cta-contract && pnpm run guard:smh-treatment-layouts"
  },
  "dependencies": {
    "micromatch": "^4.0.8"
  }
}
